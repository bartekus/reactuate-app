{
  "name": "reactuate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:desktop": "yarn workspace @reactuate/desktop start",
    "start:mobile": "yarn workspace @reactuate/mobile start",
    "mobile:ios": "yarn workspace @reactuate/mobile ios",
    "mobile:android": "yarn workspace @reactuate/mobile android",
    "test:mobile": "yarn workspace @reactuate/mobile test",
    "build:web": "yarn workspace @reactuate/web build",
    "start:web": "yarn workspace @reactuate/web start",
    "test:web": "yarn workspace @reactuate/web test",
    "start:server": "yarn workspace @reactuate/server start",
    "rebuild": "rm -rf node_modules yarn.lock packages/desktop/node_modules packages/mobile/node_modules packages/web/node_modules && yarn"
  },
