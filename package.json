    "apollo-cache-inmemory": "1.6.0",
    "apollo-client": "2.6.0",
    "apollo-link": "1.2.11",
    "apollo-link-error": "1.1.10",
    "apollo-link-http": "1.5.14",
    "fbemitter": "^2.1.1",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-apollo": "2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-native": "0.59.5",
    "styled-components": "^4.2.0",
    "url": "^0.11.0",
